<p id="notice"><%= notice %></p>

<h1>Listing Admin Devices</h1>

<table border="1">
  <thead>
    <tr>
      <th>Imei no</th>
      <th>Device type</th>
      <th>Sim no</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_devices.each do |admin_device| %>
      <tr>
        <td><%= admin_device.imei_no %></td>
        <td><%= admin_device.device_type %></td>
        <td><%= admin_device.sim_no %></td>
        <td><%= admin_device.user_id %></td>
        <td><%= link_to 'Show', admin_device %></td>
        <td><%= link_to 'Edit', edit_admin_device_path(admin_device) %></td>
        <td><%= link_to 'Destroy', admin_device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Device', new_admin_device_path %>
